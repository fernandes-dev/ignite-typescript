generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model category {
  id          String   @id @default(uuid())
  name        String
  description String?
  created_at  DateTime
}

model specification {
  id          String   @id @default(uuid())
  name        String
  description String?
  created_at  DateTime
}
